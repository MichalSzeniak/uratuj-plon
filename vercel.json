{
  "rewrites": [
    {
      "source": "/listing/:slug",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(facebookexternalhit|Twitterbot|LinkedInBot|WhatsApp)"
        }
      ],
      "destination": "/api/listing/:slug"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
